<?xml version="1.0"?>
<object name="portal_javascripts">

  <!-- plone.app.widgets sets the bundle to 'deprecated'. We set it back to
       default, it can peacefully coexist with p.a.widgets' -->
  <javascript id="++resource++plone.app.jquerytools.dateinput.js"
      bundle="default"
      enabled="True"/>

  <javascript
      id="++resource++plone.formwidget.recurrence/jquery.tmpl-beta1.js"
      cacheable="True"
      compression="safe"
      cookable="True"
      inline="False"
      enabled="True"
      />

  <javascript
      id="++resource++plone.formwidget.recurrence/jquery.recurrenceinput.js"
      insert-after="++resource++plone.formwidget.recurrence/jquery.tmpl-beta1.js"
      cacheable="True"
      compression="safe"
      cookable="True"
      inline="False"
      enabled="True"
      />

</object>
